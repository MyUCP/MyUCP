{
	"name": "myucp/myucp",
	"description": "Mini PHP framework",
	"version": "5.7.1",
	"keywords": ["myucp", "framework", "php framework"],
	"homepage": "http://myucp.ru/",
	"license": "MIT",
	"type": "project",
	"support": {
		"issues": "https://github.com/MyUCP/MyUCP/issues",
		"source": "https://github.com/MyUCP/MyUCP"
	},
	"require": {
		"php": "^7.1.3",
		"symfony/var-dumper": "^4.2",
		"vlucas/phpdotenv": "^2.2"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"MyUCP\\": "framework/",
			"Extensions\\": "extensions/"
		},
		"files": [
			"framework/Support/helpers.php"
		]
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
